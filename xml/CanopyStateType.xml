<elm_type>

    <emi_constants_prefix>STATE</emi_constants_prefix>
    <elm_mod_name>CanopyStateType</elm_mod_name>
    <elm_var_name>CanopyState_vars</elm_var_name>
    <elm_type_name>canopystate_type</elm_type_name>
    <constant_offset>600</constant_offset>

    <pack_vars>
        <column_level>
            <name>Maximum annual depth of thaw</name>
            <unit>[m]</unit>
            <emi_constant_name>ALTMAX</emi_constant_name>
            <elm_name>altmax_col</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begc</dim1_beg_name>
            <dim1_end_name>endc</dim1_end_name>
        </column_level>
        <column_level>
            <name>Prior year maximum annual depth of thaw</name>
            <unit>[m]</unit>
            <emi_constant_name>ALTMAX_LASTYEAR</emi_constant_name>
            <elm_name>altmax_lastyear_col</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begc</dim1_beg_name>
            <dim1_end_name>endc</dim1_end_name>
        </column_level>

        <patch_level>
            <name>Laminar boundary layer resistance for water over dry leaf</name>
            <unit>[s/m]</unit>
            <emi_constant_name>LBL_RSC_H2O</emi_constant_name>
            <elm_name>lbl_rsc_h2o_patch</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begp</dim1_beg_name>
            <dim1_end_name>endp</dim1_end_name>
        </patch_level>
        <patch_level>
            <name>Canopy one-sided leaf area index with burying by snow</name>
            <unit>[-]</unit>
            <emi_constant_name>ELAI</emi_constant_name>
            <elm_name>elai_patch</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begp</dim1_beg_name>
            <dim1_end_name>endp</dim1_end_name>
        </patch_level>

    </pack_vars>
    <unpack_vars>
        <patch_level>
            <name>Sunlit fraction</name>
            <unit>[-]</unit>
            <emi_constant_name>FSUN</emi_constant_name>
            <elm_name>fsun_patch</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begp</dim1_beg_name>
            <dim1_end_name>endp</dim1_end_name>
        </patch_level>
        <patch_level>
            <name>Sunlit projected leaf area index</name>
            <unit>[-]</unit>
            <emi_constant_name>LAISUN</emi_constant_name>
            <elm_name>laisun_patch</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begp</dim1_beg_name>
            <dim1_end_name>endp</dim1_end_name>
        </patch_level>
        <patch_level>
            <name>Shaded projected leaf area index</name>
            <unit>[]</unit>
            <emi_constant_name>LAISHA</emi_constant_name>
            <elm_name>laisha_patch</elm_name>
            <dimension>1</dimension>
            <is_real>1</is_real>
            <dim1_beg_name>begp</dim1_beg_name>
            <dim1_end_name>endp</dim1_end_name>
        </patch_level>

    </unpack_vars>
